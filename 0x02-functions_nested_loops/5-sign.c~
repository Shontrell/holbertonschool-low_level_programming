#include "holberton.h"
/**
 * print_sign - Entry point
 *
 * @n: int passed as argument to the function
 *
 * Return: 0
 */
int print_sign(int n)
{
	if (n > 0)
	{ _putchar (43);
		_putchar (44);
		return (1);
			}
	else if ( n == 0)
	{ _putchar ('0');
		_putchar (44);
		return (0);
	}
	else
	{ _putchar (45);
		_putchar (44);
		return (-1);
	}
}
