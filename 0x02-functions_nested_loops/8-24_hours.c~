#include "holberton.h"
/**
 * jack_bauer - Entry point
 *
 * Return: 0
 */
void jack_bauer(void)
{
	int a, b, c, d;
	while (a = '0' && a <= '2')
	{
		a++;
		while (b = '0' && b <= '9')
		{
			b++;
			while (c = '0' && c <= '5')
			{
				c++;
				while (d = '0' && d <= 9)
				{
					d++;
					if (a != '2' && b < '3')
					{
						_putchar(a);
						_putchar(b);
						_putchar(':');
						_putchar(c);
						_putchar(d);
						_putchar('\n');
					}
					else
					{
						break;
					}
				}
			}
		}
	}
}
